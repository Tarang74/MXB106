	\draw[-, very thin] \rotateTwo (\pivotTwoX, \pivotTwoY - 0.25) -- (\pivotTwoX + 0.25, \pivotTwoY - 0.25) -- (\pivotTwoX + 0.25, \pivotTwoY);

	\draw [fill] (\pivotOneX, \pivotOneY) circle [radius=0.05];
	\node[label={[label distance=0.25]90:{\scalebox{1.25}{$\symbfup{0}$}}}] at (\pivotOneX, \pivotOneY) {};

	% Right angle 1
	\draw[-, very thin] \rotateOne (\pivotOneX, \pivotOneY + 0.25) -- (\pivotOneX - 0.25,\pivotOneY + 0.25) -- (\pivotOneX - 0.25,\pivotOneY);
